function y5_defAdFormat() {
 var aFormats = ['direct', 'market', 'media', 'compatibility'];
 var sAdFormat = (window.yandex_ad_format) ? y5_checkEntry(yandex_ad_format.toString(), aFormats) : aFormats[0];
 switch (sAdFormat) {
  case aFormats[0]:
   y5_getYandexDirect();
   break;
  case aFormats[1]:
   y5_getYandexMarket();
   break;
  case aFormats[2]:
   y5_getMediaBanner();
   break;
  case aFormats[3]:
   break;
 }
}
y5_defAdFormat();
